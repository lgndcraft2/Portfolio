{
  "rewrites": [
    { "source": "/", "destination": "/home/index.html" },
    { "source": "/projects", "destination": "/projects/index.html" },
    { "source": "/:any*", "destination": "/:any*" }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/projects/index.html",
      "destination": "/projects",
      "permanent": true
    }
  ]
}